"use strict";const mcPopup=document.getElementById("mcPopup");function showMailchimpForm(){mcPopup.classList.remove("hide")}function closeMailchimpForm(){mcPopup.classList.add("hide")}mcPopup.querySelector(".close").addEventListener("click",a=>{a.preventDefault(),closeMailchimpForm()}),mcPopup.querySelector(".overlay").addEventListener("click",a=>{a.preventDefault(),closeMailchimpForm()});const newsletterLinks=document.querySelectorAll(".showNewsletterButton");newsletterLinks&&newsletterLinks.forEach(a=>{a.addEventListener("click",a=>{a.preventDefault(),showMailchimpForm()})});